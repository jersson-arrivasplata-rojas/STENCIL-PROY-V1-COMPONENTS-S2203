{
  "name": "@jersson-arrivasplata-rojas/sami",
  "version": "0.0.6",
  "description": "Componente de Stencil",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/sami/sami.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "doc:readme": "stencil build --docs-readme",
    "docs:data": "stencil build --docs-json path/to/docs.json",
    "start:max": "node --max_old_space_size=4096 ./node_modules/@stencil/core/bin/stencil build --dev --watch --serve --port=8016 --verbose --log",
    "start": "stencil build --dev --watch --serve --port=8017 --verbose --log",
    "prerender": "stencil build --prerender --dev --watch --serve --port=8016",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0"
  },
  "author": "Jersson G. Arrivasplata Rojas",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jersson-arrivasplata-rojas/STENCIL-PROY-V1-SAMI-COMPONENTS-S2203.git"
  },
  "homepage": "https://sami.jerssonarrivasplata.com/",
  "dependencies": {
    "@jersson-arrivasplata-rojas/sami-utils": "^0.0.18",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/core": "^2.13.0",
    "@stencil/react-output-target": "^0.3.1",
    "@stencil/sass": "^1.5.2",
    "@stencil/vue-output-target": "^0.6.0",
    "@stencil/webpack": "^0.0.6",
    "@types/estree": "^0.0.51",
    "@types/webrtc": "^0.0.32",
    "copy-webpack-plugin": "^10.2.4",
    "rollup-plugin-node-polyfills": "^0.2.1"
  }
}
